    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB",
            "type": "cortex-debug",
            "servertype":"openocd",
            "configFiles": ["/usr/local/share/openocd/scripts/interface/stlink.cfg",
            "/usr/local/share/openocd/scripts/target/stm32f1x.cfg"],
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/output.elf",
            "gdbpath" : "arm-none-eabi-gdb",
            "autorun": [
                "target remote localhost:3333",
                "symbol-file ./${workspaceRoot}/build/111micro.elf",
                "target reset"
                ]
        }    ]
}